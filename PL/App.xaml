<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:EngineerCollection x:Key="EngineerCollectionKey" />
        <local:EngineerCollectionWithAll x:Key="EngineerCollectionWithAll" />

        <local:ConvertIdToContent x:Key="ConverIdToContentKey"/>
        <local:ConvertIdToEnable x:Key="ConvertIdToEnable"/>
        <local:ConvertTaskToVisible x:Key="ConvertTaskToVisible"/>
        <local:ConvertTaskToEnable x:Key="ConvertTaskToEnable"/>
        <local:ConvertTaskToVisibleNot x:Key="ConvertTaskToVisibleNot"/>
        <local:ConvertBoolToEnable x:Key="ConvertBoolToEnable"/>
        <local:DateToWidthConverter x:Key="DateToWidthConverter"/>
        <local:DateToMarginConverter x:Key="DateToMarginConverter"/>
        <local:StatusToColorConverter x:Key="StatusToColorConverter"/>
        <local:GantbtnEnableConverter x:Key="GantbtnEnableConverter"/>
        <local:TextboxPlaceholderVisibilityConverter x:Key="TextboxPlaceholderVisibilityConverter"/>
        <local:ConvertValidToBorderBrush x:Key="ConvertValidToBorderBrush"/>

        <local:EmptyFieldsToEnableConverter x:Key="EmptyFieldsToEnableConverter"/>
        <local:TimeGreetingConverter x:Key="TimeGreetingConverter"/>

    </Application.Resources>
</Application>
